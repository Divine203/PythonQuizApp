#: import SlideTransition kivy.uix.screenmanager.SlideTransition

<Content>:


    Label: 
        text: "Are you sure you want to Submit?"

    MDFillRoundFlatButton:
        text: "YES"
        on_release: 
            root.current = "submit"; app.calculate_score
    
            

<DemoProject>:

    Screen:
        id: first
        name: "screen_1"

        MDIconButton:
            id: theme_btn
            icon: "icons/moon.png"
            pos_hint: {"x": .1, "y": .9}
            on_release:
                if(app.theme_cls.theme_style == "Light"):\
                app.theme_cls.theme_style = "Dark"; theme_btn.icon = "icons/sun1.png"
                else:\
                app.theme_cls.theme_style = "Light"; theme_btn.icon = "icons/moon.png"
        

        Image:
            source: "icons/logo.png"
            size_hint: (.2, .2)
            pos_hint: {"center_x": 0.5, "center_y": 0.79}
            halign: "center"

        MDLabel:
            text: "Q U I Z"
            pos_hint: {"x": 0, "y": 0.15}
            halign: "center"

        MDTextField:
            id: userID
            hint_text: 'UserID'
            pos_hint:  {"center_x": 0.5, "center_y": 0.5}
            size_hint_x: None
            width: 250

        MDTextField:
            id: password
            hint_text: 'Password'
            pos_hint:  {"center_x": 0.5, "center_y": 0.4}
            password: True
            size_hint_x: None
            width: 250

        Label:
            id: error_msg
            text: ""
            font_size: 13
            color: "red"
            pos_hint: {"x": 0, "y": -0.2}
            halign: "center"

        MDRoundFlatButton:
            text: "LogIn"
            pos_hint: {"center_x": 0.5, "center_y": 0.22}
            md_bg_color: app.theme_cls.primary_light
            on_release: 
                root.log_in(userID.text, password.text)
                if(root.log_in(userID.text, password.text)):\
                error_msg.text = ""; root.current = "screen_2"; root.transition.direction =  "left"
                else:\
                error_msg.text = "Invalid UserID/Password"

        MDLabel:
            text: "Don't have an account?"
            font_size: 13
            pos_hint: {"x": 0, "y": -0.35}
            halign: "center"

        MDFillRoundFlatButton:
            text: "Sign Up"
            pos_hint: {"center_x": 0.5, "center_y": 0.1}
            md_bg_color: app.theme_cls.primary_dark
            on_release: error_msg.text = ""; root.current = "sign_up"; root.transition.direction = "down"
    
    Screen:
        name: "sign_up"

        MDLabel:
            text: "Sign Up"
            pos_hint: {"x": 0, "y": 0.3}
            halign: "center"

        MDTextField:
            id: firstName
            hint_text: 'First Name'
            pos_hint:  {"center_x": 0.5, "center_y": 0.7}
            size_hint_x: None
            width: 250
        
        MDTextField:
            id: lastName
            hint_text: 'Last Name'
            pos_hint:  {"center_x": 0.5, "center_y": 0.6}
            size_hint_x: None
            width: 250

        MDTextField:
            id: email
            hint_text: 'Email'
            pos_hint:  {"center_x": 0.5, "center_y": 0.5}
            size_hint_x: None
            width: 250

        MDTextField:
            id: user_password
            hint_text: 'Password'
            pos_hint:  {"center_x": 0.5, "center_y": 0.4}
            size_hint_x: None
            width: 250
            password: True
        
        MDTextField:
            id: phoneNumber
            hint_text: 'Phone Number (+234)'
            pos_hint:  {"center_x": 0.5, "center_y": 0.3}
            size_hint_x: None
            width: 250

        MDRoundFlatButton:
            text: "Cancel"
            pos_hint: {"center_x": 0.3, "center_y": 0.12}
            md_bg_color: app.theme_cls.primary_light
            on_release:
